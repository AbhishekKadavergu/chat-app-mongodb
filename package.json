{
    "name": "express-hbs-starter-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "node src/index.js",
        "parcel-watch": "parcel watch templates/views/*.hbs --no-cache",
        "parcel-dev": "concurrently -k \"npm run parcel-watch\" \"npm run dev\"",
        "parcel-build": "parcel build templates/views/*.hbs --no-cache",
        "heroku-postbuild": "npm run parcel-build",
        "dev": "env-cmd -f ./config/dev.env nodemon src/index.js -e js, hbs",
        "test": "jest --runInBand --detectOpenHandles --coverage --watchAll --no-cache",
        "test:e2e": "jest --config jest.e2e.config.js --runInBand --detectOpenHandles --watchAll --no-cache",
        "lint": "eslint ./"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "dependencies": {
        "@joeattardi/emoji-button": "^4.6.0",
        "bad-words": "^3.0.4",
        "express": "^4.17.1",
        "express-hbs": "^2.3.4",
        "handlebars": "^4.7.6",
        "moment": "^2.29.1",
        "mongodb": "^3.6.3",
        "mongoose": "^5.11.14",
        "morgan": "^1.10.0",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-custom-dist-structure": "^1.1.16",
        "parcel-plugin-handlebars": "^0.4.4",
        "qs": "^6.9.6",
        "socket.io": "^3.1.0",
        "socket.io-client": "^3.1.0",
        "validator": "^13.5.2"
    },
    "devDependencies": {
        "concurrently": "^5.3.0",
        "env-cmd": "^10.1.0",
        "eslint": "^7.18.0",
        "jest": "^26.6.3",
        "jest-puppeteer": "^4.4.0",
        "livereload": "^0.9.1",
        "nodemon": "^2.0.7",
        "puppeteer": "^5.5.0",
        "supertest": "^6.1.3"
    },
    "customDistStructure": {
        "config": {
            "js": [
                ".js",
                ".js.map"
            ],
            "img": [
                ".jpg",
                ".png",
                ".ico"
            ],
            "css": [
                ".css",
                ".css.map"
            ]
        },
        "options": {
            "development": true
        }
    }
}
